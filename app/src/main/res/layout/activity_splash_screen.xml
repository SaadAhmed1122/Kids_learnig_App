<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/park"
    tools:context=".SplashScreen">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="151dp"
        android:layout_height="216dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cartoon_boy_playing_with_building_blocks_premium_vector_png_removebg_preview" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="275dp"
        android:layout_height="206dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="45dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView16"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/png_clipart_boy_playing_with_toys_and_crayons_child_toy_play_cartoon_kids_toys_game_people_removebg_preview" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="183dp"
        android:layout_height="103dp"
        android:layout_marginStart="115dp"
        android:layout_marginTop="24dp"
        android:onClick="showmain"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView16"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView13"
        app:srcCompat="@drawable/pngtree_start_game_button_icon_cartoon_png_image_2135297_removebg_preview" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/unnamed_removebg_preview" />

</androidx.constraintlayout.widget.ConstraintLayout>